<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>4k Movies</name>
    <match>all</match>
    <rule field="videoresolution" operator="greaterthan">
        <value>1080</value>
    </rule>
	<limit>30</limit>
	 <order direction="descending">random</order>
</smartplaylist>
